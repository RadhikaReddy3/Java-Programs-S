package com.lazy;

public class STonLazyLoad {
	
	private static STonLazyLoad sl = null;
	
	private STonLazyLoad() {
		
	}
	
	public static STonLazyLoad getsingletonObj() {
		
		if(sl==null) {
			
			System.out.println("I am inside if");
			sl = new STonLazyLoad();
			return sl;
		}
		else return sl;
		
	}

}
