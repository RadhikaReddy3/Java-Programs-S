package com.cisco;

public class Student {
	
	private int sid;
	public int getSid() {
		return sid;
	}
	public void setSid(int sid) {
		this.sid = sid;
	}
	public String getSname() {
		return sname;
	}
	public void setSname(String sname) {
		this.sname = sname;
	}
	public double getSage() {
		return sage;
	}
	public void setSage(double sage) {
		this.sage = sage;
	}
	private String sname;
	private double sage;
	

}
