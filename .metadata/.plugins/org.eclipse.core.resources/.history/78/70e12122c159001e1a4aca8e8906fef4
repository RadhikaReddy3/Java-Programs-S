package com.cisco;

public class Employeede {
               
	      public static void main(String[] args) {
	    	  
	    	  //dec of var
	    	  int eid;
	    	  String ename;
	    	  double esal;
	    	  String edesig;
	    	  String edept;
	    	  char egen;
	    	  boolean emstatus;
	    	  long ephno;
	    	  
	    	  //initi of var
	    	  
	    	  eid=123;
	    	  ename="ram";
	    	  esal=234451;
	    	  edesig="se";
	    	  edept="IT";
	    	  egen='m';
	    	  emstatus=false;
	    	  ephno=9378378229l;
	    	  
	    	  //print
	    	  
	    	  System.out.println("eid"+eid);
	    	  System.out.println("ename"+ename);
	    	  System.out.println("esal"+esal);
	    	  System.out.println("edesig"+edesig);
	    	  System.out.println("edept"+edept);
	    	  System.out.println("egen"+egen);
	    	  System.out.println("emstatus"+emstatus);
	    	  System.out.println("ephno"+ephno);
	    	  
	    	  //initi of var 2 
	    	  
	    	  eid=124;
	    	  ename="sita";
	    	  esal=234452;
	    	  edesig="jd";
	    	  edept="IT";
	    	  egen='f';
	    	  emstatus=false;
	    	  ephno=9378378339l;
	    	  
	    	  //print
	    	  
	    	  System.out.println("eid"+eid);
	    	  System.out.println("ename"+ename);
	    	  System.out.println("esal"+esal);
	    	  System.out.println("edesig"+edesig);
	    	  System.out.println("edept"+edept);
	    	  System.out.println("egen"+egen);
	    	  System.out.println("emstatus"+emstatus);
	    	  System.out.println("ephno"+ephno);
	    	  
	    	  
	      }
}
