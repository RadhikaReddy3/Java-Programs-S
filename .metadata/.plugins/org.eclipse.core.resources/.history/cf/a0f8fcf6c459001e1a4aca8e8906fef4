package com.cisco;

public class StudentRegistration {

	public static void main(String[] args) {
		//Declaration Of First student
		 byte sid;
		 String sname;
		 double sper;
		 String sloc;
		 String squal;
		 char sgen;
		 String sadd;
		 byte sage;
		 String smail;
		//Initialization Of First Student
		 sid=1;
		 sname="radhika";
		 sper=92;
	     sloc="hyd";
		 squal="B.TECH";
		 sgen='F';
		 sadd="hyd";
		 sage=21;
		 smail="a@gmail.com\n\n\n\n\n\n\t\rn";
		//print
		 System.out.println("Student ID: "+sid);
   	     System.out.println("Student Name: "+sname);
   	     System.out.println("Student percentage: "+sper);
   	     System.out.println("Student location: "+sloc);
   	     System.out.println("Student qualification: "+squal);
   	     System.out.println("Student gender: "+sgen);
   	     System.out.println("Student address: "+sadd);
   	     System.out.println("Student age: "+sage);
   	     System.out.println("Student mail: "+smail);
   	     
   	     //Initialization of Second Student
   	     sid=2;
		 sname="rani";
		 sper=93;
	     sloc="wrgl";
		 squal="B.com";
		 sgen='F';
		 sadd="nagole";
		 sage=22;
		 smail="b@gmail.com";
		 //print
		 System.out.println("Student ID: "+sid);
   	     System.out.println("Student Name: "+sname);
   	     System.out.println("Student percentage: "+sper);
   	     System.out.println("Student location: "+sloc);
   	     System.out.println("Student qualification: "+squal);
   	     System.out.println("Student gender: "+sgen);
   	     System.out.println("Student address: "+sadd);
   	     System.out.println("Student age: "+sage);
   	     System.out.println("Student mail: "+smail);
   	  

	}

}
